<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="301dp">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/first"
                    android:layout_width="100dp"
                    android:layout_height="100dp"

                    android:background="#51019385" />

                <ImageView
                    android:id="@+id/second"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:background="#FFFFFF" />

                <ImageView
                    android:id="@+id/third"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:background="#51019385" />

                <Button
                    android:id="@+id/reset"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="reset"
                    android:textAlignment="center" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/forth"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:background="#FFFFFF" />

                <ImageView
                    android:id="@+id/fifth"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:background="#51019385" />

                <ImageView
                    android:id="@+id/sixth"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:background="#FFFFFF" />

                <Button
                    android:id="@+id/cancel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="cancel" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/seventh"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:background="#51019385" />

                <ImageView
                    android:id="@+id/eighth"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:background="#FFFFFF" />

                <ImageView
                    android:id="@+id/ninth"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:background="#51019385" />

                <Button
                    android:id="@+id/apply"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="apply" />
            </TableRow>

        </TableLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <Button
                android:id="@+id/first_palette"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="First Palette" />

            <Button
                android:id="@+id/alter"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="alter" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <Button
                android:id="@+id/db1"
                android:layout_width="136dp"
                android:layout_height="136dp"
                android:text="Button"
                android:background="@drawable/art1" />

            <Button
                android:id="@+id/db2"
                android:layout_width="136dp"
                android:layout_height="136dp"
                android:text="Button"
                android:background="@drawable/art2"/>

            <Button
                android:id="@+id/db3"
                android:layout_width="136dp"
                android:layout_height="136dp"
                android:text="Button"
                android:background="@drawable/art3"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <Button
                android:id="@+id/db4"
                android:layout_width="136dp"
                android:layout_height="136dp"
                android:text="Button"
                android:background="@drawable/art4"/>

            <Button
                android:id="@+id/db5"
                android:layout_width="136dp"
                android:layout_height="136dp"
                android:text="Button"
                android:background="@drawable/art5"/>

            <Button
                android:id="@+id/db6"
                android:layout_width="136dp"
                android:layout_height="136dp"
                android:text="Button"
                android:background="@drawable/art6"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="134dp"
            android:orientation="horizontal">

            <Button
                android:id="@+id/db7"
                android:layout_width="136dp"
                android:layout_height="136dp"
                android:background="@drawable/art7"
                android:text="Button" />

            <Button
                android:id="@+id/db8"
                android:layout_width="136dp"
                android:layout_height="136dp"
                android:background="@drawable/art8"
                android:text="Button" />

            <Button
                android:id="@+id/db9"
                android:layout_width="136dp"
                android:layout_height="136dp"
                android:background="@drawable/art9"
                android:text="Button" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <Button
                android:id="@+id/db10"
                android:layout_width="136dp"
                android:layout_height="136dp"
                android:background="@drawable/art1"
                android:text="Button" />

            <Button
                android:id="@+id/db11"
                android:layout_width="136dp"
                android:layout_height="136dp"
                android:background="@drawable/art2"
                android:text="Button" />

            <Button
                android:id="@+id/db12"
                android:layout_width="136dp"
                android:layout_height="136dp"
                android:background="@drawable/art3"
                android:text="Button" />
        </LinearLayout>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>